{
	"parser": "babel-eslint",
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"env": {
		"browser": true,
		"commonjs": true,
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"globals": {
		"App": "readonly",
		"cn": "readonly",
		"t": "readonly",
		"td": "readonly",
		"html": "readonly",
		"fm": "readonly"
	},
	"rules": {
		"react/no-deprecated": 0,
		"react/display-name": 0,
		"react/prop-types": 0,
		"react/no-string-refs": 0,
		"react/no-find-dom-node": 0,
		"no-mixed-spaces-and-tabs": 0,
		"no-case-declarations": 0,
		"indent": 0,
		"no-tabs": 0,
		"no-console": [1, { "allow": ["info", "warn", "error"] }],
		"no-debugger": 0,
		"no-unused-vars": [1, { "ignoreRestSiblings": true }],
		"strict": ["error", "global"],
		"curly": 1,
		"no-undef": 0
	}
}
